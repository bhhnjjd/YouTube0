{
  "name": "youtube-auto-editor",
  "version": "1.0.0",
  "description": "YouTube全自动化视频制作和发布系统",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js --web",
    "dev": "nodemon src/index.js --web",
    "cli": "node src/index.js",
    "build": "docker build -t youtube-automation .",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up -d",
    "lint": "eslint src/",
    "test": "jest"
  },
  "keywords": ["youtube", "automation", "video", "editing", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^126.0.1",
    "puppeteer": "^21.5.0",
    "winston": "^3.11.0",
    "youtube-dl-exec": "^2.4.13",
    "node-cron": "^3.0.2",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  }
